{
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "plugins": [
        "prettier-plugin-css-order",
        "@ianvs/prettier-plugin-sort-imports"
    ],
    "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
    "importOrderTypeScriptVersion": "5.0.0",
    "importOrder": [
        "<BUILTIN_MODULES>",
        "<THIRD_PARTY_MODULES>",
        "",
        "^(?!.*[.][s]*css$)[.][.]",
        "",
        "^(?!.*[.][s]*css$)[./].*$",
        "",
        ".[s]*css$"
    ]
}
